{
  "expo": {
    "name": "Ethrift Mongolia",
    "slug": "Ethrift-mob",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ehtift_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.temuulennn.Ethriftmob",
      "buildNumber": "5"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.temuulennn.Ethriftmob",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/ehtift_icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "clerkPublishableKey": "pk_test_cG9saXRlLWNob3ctODAuY2xlcmsuYWNjb3VudHMuZGV2JA",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3d09230b-c8ca-4bb7-83cf-b6dcdf0f8427"
      }
    }
  }
}
